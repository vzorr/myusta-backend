{
  "name": "myusta-backend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "sequelize": "sequelize",
    "db:migrate": "sequelize db:migrate --env $NODE_ENV --config src/config/config.js --migrations-path src/migrations",
    "db:seed": "sequelize db:seed:all --env $NODE_ENV --config src/config/config.js --seeders-path src/seeders",
    "db:rollback": "sequelize db:migrate:undo --env $NODE_ENV --config src/config/config.js --migrations-path src/migrations"
  },
  "sequelize": {
    "config": "src/config/config.js",
    "models-path": "src/models",
    "migrations-path": "src/migrations",
    "seeders-path": "src/seeders"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "ejs": "^3.1.10",
    "express": "^5.1.0",
    "google-auth-library": "^9.15.1",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "nodemailer": "^6.10.0",
    "pg": "^8.14.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.9",
    "sequelize-cli": "^6.6.2"
  }
}
